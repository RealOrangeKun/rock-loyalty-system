{
  "AllowedHosts": "*",
  "JwtOptions": {
    "ExpirationInMinutes": 30,
    "JwtCookieOptions": {
      "HttpOnly": true,
      "Secure": true,
      "SameSite": "strict"
    }
  },
    "IpRateLimiting": {
      "EnableEndpointRateLimiting": true,
      "StackBlockedRequests": false,
      "RealIpHeader": "X-Real-IP",
      "ClientIdHeader": "X-ClientId",
      "HttpStatusCode": 429,
      "GeneralRules": [
        {
          "Endpoint": "*",
          "Period": "1m",
          "Limit": 100
        }
      ]
    }
}