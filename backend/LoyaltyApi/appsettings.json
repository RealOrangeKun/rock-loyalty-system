{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "ExpirationInMinutes": 30,
    "JwtCookieOptions": {
      "HttpOnly": true,
      "Secure": true,
      "SameSite": "strict"
    }
  }
}